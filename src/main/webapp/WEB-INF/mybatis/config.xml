<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- jdbc CamelCase 변환 파라메터 NULL 존재시 varchar변환 -->
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true" />
		<setting name="jdbcTypeForNull" value="VARCHAR" />
	</settings>
	<!-- Vo클래스 별칭지정 -->
	<typeAliases>
		<typeAlias type="com.happypet.animal.Entity.AnimalVo"
			alias="animalVo" />
		<typeAlias type="com.happypet.animal.Entity.FileBoardVo"
			alias="fileBoardVo" />

		<!-- 마켓 -->
		<typeAlias
			type="com.happypet.animal.Entity.MarketEntity.MarketVo"
			alias="marketVo" />
		<typeAlias
			type="com.happypet.animal.Entity.MarketEntity.MarketFileVo"
			alias="marketFileVo" />
		<typeAlias
			type="com.happypet.animal.Entity.MarketEntity.ConbineMarket"
			alias="ConbineMarketVo" />
		<typeAlias
			type="com.happypet.animal.Entity.MarketEntity.MarketCart"
			alias="marketCartVo" />
		<typeAlias type="com.happypet.animal.Entity.MarketEntity.MarketCartView" alias="marketCartViewVo" />
		<typeAlias type="com.happypet.animal.Entity.MarketEntity.MarketCartOrderVo" alias="marketCartOrderVo" />
		<!--  -->


		<typeAlias type="com.happypet.animal.Entity.AccountVo"
			alias="account" />
		<typeAlias type="com.happypet.animal.Entity.HospitalVo"
			alias="hospital" />
		<typeAlias
			type="com.happypet.animal.Entity.HospitalReviewVo" alias="hr" />
	</typeAliases>
</configuration> 
